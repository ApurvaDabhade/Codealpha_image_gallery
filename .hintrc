{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-wrap",
          "-webkit-text-size-adjust"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.3",
    "not safari <= 18.3",
    "not chrome <= 109",
    "not firefox <= 115"
  ]
}